{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "es2015",
      "es2015.promise",
      "es2016.array.include",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@cuteapp/*": [
        "App",
        "App/*"
      ]
    },
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "esnext",
    "module": "esnext"
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
